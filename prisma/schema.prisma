generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model a_user_role {
  role_id          String   @id
  role_name        String
  transaction_date DateTime
}

model a_user_system {
  id_user          String   @id
  transaction_date DateTime?
  nik              String?  
  username         String?  
  password         String?  
  role_id          String?  
  office_email     String?  
  department_code  String?  
  is_active        String?  
}

model a_department {
  department_id   String    @id 
  department_code String?   @unique 
  department_name String?
  created_at      DateTime?
}
